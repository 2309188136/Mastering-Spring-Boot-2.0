spring:
  application:
    name: customer-service
  cloud:
    stream:
      kafka:
        binder:
          brokers:
          - localhost:9092

server:
  port: 6161

eureka:
  client:
    service-url:
      default-zone: ${EUREKA_URI:http://localhost:8761/eureka}
  instance:
    prefer-ip-address: true

logging:
    file: customer.log
    level:
      root: 
      project:
          user:
            UserClient: FULL
