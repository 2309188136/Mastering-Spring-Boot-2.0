package com.dineshonjava.accountservice.repository;

import java.util.List;

import org.springframework.data.repository.CrudRepository;

import com.dineshonjava.accountservice.domain.Account;

/**
 * @author Dinesh.Rajput
 *
 */
public interface AccountRepository extends CrudRepository<Account, Integer>{
	
	List<Account> findByCustomer_id(Integer customer_id);
	
	List<Account> findAllByAccount_type(String account_type);
	
	List<Account> findByBank(String bank);
	
	Account findAccountByAccount_id(Integer account_id);
}
