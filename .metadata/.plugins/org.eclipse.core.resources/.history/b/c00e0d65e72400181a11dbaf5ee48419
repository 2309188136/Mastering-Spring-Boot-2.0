package com.dineshonjava.customerservice.repository;

import java.util.List;

import org.springframework.data.repository.CrudRepository;

import com.dineshonjava.customerservice.domain.Customer;

/**
 * @author Dinesh.Rajput
 *
 */
public interface CustomerRepository extends CrudRepository<Customer, Integer>{
	
	List<Customer> findAllByCustomerId(Integer customerId);
	
	List<Customer> findAllByAccountType(String accountType);
	
	List<Customer> findByBank(String bank);
	
	Customer findAccountByAccountId(Integer accountId);
}
